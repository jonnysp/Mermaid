{
    "name": "jonnysp/mermaid",
    "type": "contao-bundle",
    "description": "Adds Mermaid Diagramms to Contao 4",
    "keywords":["mermaid","diagramm", "contao"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Jonny Spitzner",
            "homepage": "https://github.com/jonnysp"
        }
    ],
    "require": {
        "php":">=5.6.0",
        "contao/core-bundle":"^4.4"
    },
    "conflict": {
         "contao/core-bundle": "<4.4.1"
    },
    "autoload": {
        "psr-4": {
            "Jonnysp\\Mermaid\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "Jonnysp\\Mermaid\\ContaoManager\\Plugin"
    },
    "support": {
        "issues": "https://github.com/jonnysp/Mermaid/issues",
        "source": "https://github.com/jonnysp/Mermaid"
    }
}
